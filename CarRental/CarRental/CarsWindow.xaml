<Window x:Class="CarRental.CarsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarRental"
        mc:Ignorable="d"
        Title="CarsWindow" Height="450" Width="800">
    <Grid>
        <ListView Name="carlist" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="303,88,135,78">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Label Content="CARS LIST" HorizontalAlignment="Left" Margin="434,46,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.513,0.421" FontSize="20"/>
        <Button Name="add" Content="ADD" HorizontalAlignment="Left" Margin="104,97,0,0" VerticalAlignment="Top" Width="50" Height="45" />
        <Button Name="remove" Content="REMOVE" HorizontalAlignment="Left" Margin="104,163,0,0" VerticalAlignment="Top" Height="45" />
        <Button Name="back" Content="BACK" HorizontalAlignment="Left" Margin="104,232,0,0" VerticalAlignment="Top" Height="45" Width="50"/>
        <Button x:Name="update" Content="UPDATE" HorizontalAlignment="Left" Margin="104,297,0,0" VerticalAlignment="Top" Height="45" Width="50"/>

    </Grid>
</Window>
